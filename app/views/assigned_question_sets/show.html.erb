<%- model_class = AssignedQuestionSet -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:question_set_id) %>:</strong></dt>
  <dd><%= @assigned_question_set.question_set_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:survey_id) %>:</strong></dt>
  <dd><%= @assigned_question_set.survey_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:position) %>:</strong></dt>
  <dd><%= @assigned_question_set.position %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              assigned_question_sets_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_assigned_question_set_path(@assigned_question_set), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              assigned_question_set_path(@assigned_question_set),
              :method => 'delete',
              :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')),
              :class => 'btn btn-danger' %>
</div>
