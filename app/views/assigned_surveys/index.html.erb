<%- model_class = AssignedSurvey -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:survey_id) %></th>
      <th><%= model_class.human_attribute_name(:division_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @assigned_surveys.each do |assigned_survey| %>
      <tr>
        <td><%= link_to assigned_survey.id, assigned_survey_path(assigned_survey) %></td>
        <td><%= assigned_survey.survey_id %></td>
        <td><%= assigned_survey.division_id %></td>
        <td><%=l assigned_survey.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_assigned_survey_path(assigned_survey), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      assigned_survey_path(assigned_survey),
                      :method => :delete,
                      :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')),
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_assigned_survey_path,
            :class => 'btn btn-primary' %>
