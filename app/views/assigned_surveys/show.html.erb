<%- model_class = AssignedSurvey -%>
<div class="page-header clearfix">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:survey_id) %>:</strong></dt>
  <dd><%= @assigned_survey.survey_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:division_id) %>:</strong></dt>
  <dd><%= @assigned_survey.division_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              assigned_surveys_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_assigned_survey_path(@assigned_survey), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              assigned_survey_path(@assigned_survey),
              :method => 'delete',
              :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')),
              :class => 'btn btn-danger' %>
</div>
